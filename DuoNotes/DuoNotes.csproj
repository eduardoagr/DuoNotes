		<PackageReference Include="Syncfusion.Xamarin.SfRichTextEditor" Version="19.4.0.55" />
		<PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
		<PackageReference Include="Xamarin.CommunityToolkit" Version="2.0.0" />
		<PackageReference Include="Xamarin.FFImageLoading.Svg.Forms" Version="2.4.11.982" />
		<PackageReference Include="Xamarin.Forms" Version="5.0.0.2337" />
		<PackageReference Include="Xamarin.Essentials" Version="1.7.1" />
		<PackageReference Include="Xamarin.Forms.BehaviorsPack" Version="3.0.0" />
		<PackageReference Include="Xamarin.Forms.PancakeView" Version="2.3.0.759" />
		<PackageReference Include="Xamarin.Forms.Visual.Material" Version="5.0.0.2337" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Pages\NotebooksPage.xaml.cs">
		  <DependentUpon>NotebooksPage.xaml</DependentUpon>
		</Compile>
		<Compile Update="Pages\PopUps\NotebookPopUpPage.xaml.cs">
		  <DependentUpon>NotebookPopUpPage.xaml</DependentUpon>
		</Compile>
		<Compile Update="Pages\PopUps\NotesPopUpPage.xaml.cs">
		  <DependentUpon>NotesPopUpPage.xaml</DependentUpon>
		</Compile>
		<Compile Update="Pages\PopUps\RegisterPopUpPage.xaml.cs">
		  <DependentUpon>RegisterPopUpPage.xaml</DependentUpon>
		</Compile>
		<Compile Update="Resources\AppResources.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>AppResources.resx</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="Pages\EditorPage.xaml">
		  <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
		</EmbeddedResource>
		<EmbeddedResource Update="Pages\SplashScreenPage.xaml">
		  <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
		</EmbeddedResource>
		<EmbeddedResource Update="Resources\AppResources.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>AppResources.Designer.cs</LastGenOutput>
		</EmbeddedResource>
		<EmbeddedResource Update="Pages\LoginPage.xaml">
			<Generator>MSBuild:UpdateDesignTimeXaml</Generator>
		</EmbeddedResource>
		<EmbeddedResource Update="Pages\NotesPage.xaml">
		  <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
		</EmbeddedResource>
		<EmbeddedResource Update="Pages\PopUps\NotebookPopUpPage.xaml">
			<Generator>MSBuild:UpdateDesignTimeXaml</Generator>
		</EmbeddedResource>
		<EmbeddedResource Update="Pages\PopUps\NotesPopUpPage.xaml">
